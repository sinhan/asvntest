#!/bin/sh
USAGE=$0" [-f property-file]"

PROPERTIES=`dirname $0`/jmeter.properties

while getopts h:f: x; do 
    case $x in
	f) PROPERTIES=$OPTARG;;
	?) echo $USAGE; exit -1;;
    esac;
done

java -classpath $CLASSPATH:`dirname $0`/../lib/xerces.jar:`dirname $0`/ApacheJMeter.jar org.apache.jmeter.RemoteDriver $PROPERTIES
